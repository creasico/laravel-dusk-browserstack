{
    "name": "creasi/dusk-browserstack",
    "description": "Additional BrowserSstack Supports for Laravel Dusk",
    "keywords": ["laravel", "package", "dusk", "browserstack"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Creasi Developers",
            "email": "developers@creasi.co"
        }
    ],
    "support": {
        "source": "https://github.com/creasico/laravel-dusk-browserstack"
    },
    "scripts": {
        "fix": [
            "pint --preset laravel"
        ]
    },
    "autoload": {
        "psr-4": {
            "Creasi\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Creasi\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/dusk": "^7.0",
        "phpunit/phpunit": "^10.0.1"
    },
    "require-dev": {
        "laravel/pint": "^1.1"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {}
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
